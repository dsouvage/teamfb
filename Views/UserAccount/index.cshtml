@model IEnumerable<teamfb.Models.UserAccount>

@{
    ViewBag.Title = "Index";
}

<h2>User @ViewBag.Name</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BusinessAcountID)
        </th>
        <th></th>
    </tr>
    @if (Model == null)
    {
        <p>Not signed in</p>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BusinessAcountID)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.BusinessAcountID }) |
                </td>
            </tr>
        }
    }


</table>